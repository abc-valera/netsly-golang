  "version": "0.2.0",
  "configurations": [
    {
      "name": "HTTP Debug Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/http",
      "env": { "CONFIG_PATH": "${workspaceFolder}/.debug.env" },
      "envFile": "${workspaceFolder}/.debug.env"
    },
    {
      "name": "HTMX Debug Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/htmx",
      "env": { "CONFIG_PATH": "${workspaceFolder}/.debug.env" },
      "envFile": "${workspaceFolder}/.debug.env"
    }
  ]
}
